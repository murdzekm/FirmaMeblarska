@model FirmaMeblarska.Models.ZespolPracownik

@{
    ViewData["Title"] = "CreateZespolPracownik";
}
<h1>CreateZespolPracownik</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="PracownikId" class="control-label"></label>
                <select asp-for="PracownikId" class="form-control" asp-items="ViewBag.PracownikId"></select>
            </div>

            @Html.ListBox("availOptions", (MultiSelectList)ViewBag.availOpts, new { @size = 10, @class = "form-control" })

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>




